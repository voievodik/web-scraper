{
  "version": 2,
  "env": {
    "MARYLAND_GOVERNMENT_BIDS_PORTAL": "https://emma.maryland.gov/page.aspx/en/rfp/request_browse_public"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
